{
  "version": 2,
  "builds": [{
    "src": "index.js",
    "use": "@vercel/node",
    "config": {
      "maxLambdaSize": "50mb",
      "includeFiles": ["src/**", ".env"]
    }
  }],
  "routes": [{
    "src": "/api/.*",
    "dest": "index.js",
    "headers": {
      "Access-Control-Allow-Origin": "https://genuine-front-rho.vercel.app",
      "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
      "Access-Control-Allow-Headers": "Content-Type,Authorization"
    }
  }]
}